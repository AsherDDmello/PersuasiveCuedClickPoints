connect system/system;

 create table cuepoints_registration(fullname varchar2(64) NOT NULL,
 loginname varchar2(32) NOT NULL,role varchar2(32) NOT NULL,question varchar2(128) NOT NULL,
 answer varchar2(128) NOT NULL,cuepoints varchar2(128),pct1 blob,
 pct2 blob,pct3 blob,pct4 blob,pct5 blob,blocks varchar2(128),choiceno number(3));

